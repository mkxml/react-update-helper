{
  "name": "react-update-helper",
  "version": "1.0.0",
  "description": "Precise React re-renders with debug and immutable support",
  "scripts": {
    "lint": "eslint karma.conf.js tests.webpack.js src __tests__",
    "test": "npm run lint && karma start karma.conf.js --single-run",
    "compile": "npm run lint && npm run test && babel src -d lib",
    "build": "npm run compile && npm run docs",
    "watch-test": "karma start karma.conf.js --auto-watch --no-single-run",
    "docs": "esdoc -c esdoc.json",
    "ci": "npm run test && coveralls < coverage/lcov.info"
  },
  "author": "Matheus Kautzmann <matheus@fluidrobot.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "react-helper",
    "react-update-helper",
    "shallow compare",
    "immutable",
    "debug"
  ],
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.0",
    "babel-plugin-transform-class-properties": "6.8.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-airbnb": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "debug": "2.2.0",
    "enzyme": "2.2.0",
    "esdoc": "0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.0",
    "eslint-config-airbnb-base": "3.0.0",
    "eslint-plugin-import": "1.7.0",
    "eslint-plugin-jsx-a11y": "1.2.0",
    "eslint-plugin-react": "5.0.1",
    "immutable": "3.8.1",
    "isparta-instrumenter-loader": "1.0.0",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-coverage": "1.0.0",
    "karma-mocha": "1.0.1",
    "karma-mocha-reporter": "2.0.3",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "phantomjs-prebuilt": "2.1.7",
    "react": "15.0.2",
    "react-addons-test-utils": "15.0.2",
    "react-dom": "15.0.2",
    "sinon": "1.17.4",
    "webpack": "1.13.0"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "immutable": ">=3.8.0",
    "debug": ">=2.2.0"
  },
  "dependencies": {
    "lodash.keys": "4.0.6",
    "lodash.union": "4.3.0"
  }
}
